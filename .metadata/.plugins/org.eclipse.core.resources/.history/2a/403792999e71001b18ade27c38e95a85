
public class Subsequence {
	public static void main (String [] args) {
		int[] array = {1,3,5,6,7,2,3,5,7,8,9,11};
		int[] copyArray = new int[100];
		int count = 0;
		int max = 0;
		int j = 0;
		int i = 1;
		
		for(i = 1; i < array.length; i++) {
			for (j = 0; j < i; j++) {
				if (array[i] > array[j])
				{
						count +=1;
						if (count == 0) {
							copyArray[i] = array[i];
						}
						
				}	
			}
		}
		for (int k = 0; k < copyArray.length; k++)
		{
		  System.out.print(copyArray[k]);
		  System.out.print(",");
		}
	}
	
	
}
